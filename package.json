{
  "name": "iag-tesseract-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:open": "start http://localhost:3000 && pnpm run dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "check": "pnpm lint && pnpm check:format",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:view": "start coverage/lcov-report/index.html",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.1",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.30.1",
    "@storybook/addon-a11y": "9.0.16",
    "@storybook/addon-docs": "9.0.16",
    "@storybook/addon-onboarding": "9.0.16",
    "@storybook/addon-vitest": "9.0.16",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitest/coverage-istanbul": "3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "9.0.16",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "storybook": "9.0.16",
    "tailwindcss": "^4",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "typescript-eslint": "^8.36.0",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "@storybook/nextjs-vite": "^9.0.16",
    "@vitest/browser": "3.2.4",
    "playwright": "^1.54.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "eslint --fix",
      "prettier --write"
    ]
  }
}
